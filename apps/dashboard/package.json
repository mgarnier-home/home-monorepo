    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve-handler": "^6.1.5",
    "ts-node": "^10.9.1",
    "vite": "^5.0.10"
  }
}