{
  "private": true,
  "name": "dashboard",
  "scripts": {
    "build": "run-s build:*",
    "build:app": "ng build",
    "build:server": "webpack build --config server/webpack.config.ts",
    "dev": "run-p dev:*",
    "dev:app": "ng serve --port 3001 --host 0.0.0.0",
    "dev:server": "webpack build --config server/webpack.config.ts --mode development --watch",
    "test:app": "ng test",
    "start": "npm run build && node --env-file ../../.env dist/server/main.js"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^19.2.12",
    "@angular/animations": "^19.2.11",
    "@angular/cli": "^19.2.12",
    "@angular/common": "^19.2.11",
    "@angular/compiler": "^19.2.11",
    "@angular/compiler-cli": "^19.2.11",
    "@angular/core": "^19.2.11",
    "@angular/forms": "^19.2.11",
    "@angular/platform-browser": "^19.2.11",
    "@angular/platform-browser-dynamic": "^19.2.11",
    "@angular/router": "^19.2.11",
    "@types/jasmine": "~5.1.4",
    "@types/ping": "^0.4.4",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ping": "^0.4.4",
    "rxjs": "~7.8.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwindcss": "^3.4.6",
    "tslib": "^2.6.3",
    "zod": "^3.23.8",
    "zone.js": "0.15.0"
