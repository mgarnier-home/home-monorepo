{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon --require tsconfig-paths/register server/main.ts -w server",
    "start": "npm run build && cross-env NODE_ENV=production node server-dist/server/main.js",
    "build": "shx rm -rf app-dist dist && npm run build-app && npm run build-server",
    "build-app": "vite build --emptyOutDir",
    "build-server": "tsc"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^3.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "module-alias": "^2.2.3",
    "ping": "^0.4.4",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "utils": "workspace:^",
    "vite": "^4.0.4",
    "vite-express": "*"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/js-yaml": "^4.0.6",
    "@types/node": "^18.11.18",
    "@types/ping": "^0.4.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.16",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.30",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.9.0",
    "shx": "^0.3.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.3"
  },
  "_moduleAliases": {
    "@shared": "shared/dist"
  }
}
