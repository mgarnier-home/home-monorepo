{
  "private": true,
  "name": "dashboard",
  "scripts": {
    "patch-version": "npm version patch",
    "build": "run-s build:*",
    "dev": "run-p dev:*",
    "esbuild:server": "tsx scripts/server.ts",
    "lint:server": "tsc --project server/tsconfig.json --noEmit",
    "build:app": "ng build",
    "build:server": "npm run lint:server && npm run esbuild:server -- build",
    "dev:app": "ng serve --port 3001",
    "dev:server": "tsx watch --tsconfig server/tsconfig.json server/src/main.ts",
    "start:app": "run-p \"esbuild:app -- serve\" dev:app:tailwind",
    "start:server": "npm run build:server && node server-dist/main.js",
    "test:app": "ng test"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^17.3.1",
    "@angular/animations": "^17.3.0",
    "@angular/cli": "^17.3.1",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/ping": "^0.4.4",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jasmine-core": "~5.1.0",
    "js-yaml": "^4.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "logger": "workspace:*",
    "nodesight-types": "workspace:*",
    "ping": "^0.4.4",
    "rxjs": "~7.8.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.3.0",
    "utils": "workspace:*",
    "zone.js": "~0.14.3"
  },
  "version": "2.0.3"
}
