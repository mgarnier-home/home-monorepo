{
  "private": true,
  "name": "stats-api",
  "scripts": {
    "esbuild": "tsx scripts/build.ts",
    "lint": "tsc --noEmit",
    "build": "npm run lint && npm run esbuild -- build",
    "dev": "run-p dev:*",
