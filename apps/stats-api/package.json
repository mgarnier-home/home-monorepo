    "dev:esbuild": "npm run esbuild -- dev",
    "start": "npm run build && node dist/main.js"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.33.2",
    "@types/express": "^4.17.21",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "nodesight-types": "workspace:*",
    "utils": "workspace:*"
  }
}
