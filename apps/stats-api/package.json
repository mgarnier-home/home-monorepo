{
  "name": "stats-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -p [{name}] -n \"TSC,NODE\" -c \"yellow.bold,cyan.bold\" \"npm:compile-w\" \"npm:start-w\"",
    "compile-w": "npm run compile -- --watch --preserveWatchOutput",
    "start-w": "npm run sleep2.5s && nodemon --watch dist --delay 1 dist/index.js",
    "sleep2.5s": "node -e \"setTimeout(() => process.exit(0), 2500)\"",
    "compile": "shx rm -rf dist/ && tsc",
    "start": "npm run compile && node dist/"
  },
  "keywords": [],
  "author": "",
  "type": "module",
  "license": "ISC",
  "devDependencies": {
    "utils": "workspace:^",
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.2",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "shx": "^0.3.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.33.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1"
  }
}
